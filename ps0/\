\begin{problems}

\problem  % Problem 1

\begin{flalign*}
    A &= \{1, 6, 12, 13, 9\}&& \\
        B &= \{3, 6, 12, 15\}&&
\end{flalign*}
\begin{problemparts}
\problempart % Problem 1a
    \begin{flalign*}
        A \cap B = \{6, 12\}&&
    \end{flalign*}
\problempart % Problem 1b
    \begin{flalign*}
        A \cup B &= \{1, 6, 9, 12, 13, 9, 15\} & \\
        \vert A \cap B \vert &= 7 &&
    \end{flalign*}
\problempart % Problem 1c
    \begin{flalign*}
        A - B &= \{1, 12, 9\}&\\
        \vert A - B \vert &= 3 &&
    \end{flalign*}
\end{problemparts}

\problem  % Problem 2

\begin{flalign*}
    X & \in \{0, 1, 2, 3\} &\\  
    Y & \in \{1, 2, 3, 4, 5, 6, 8, 9, 10, 12, 15, 16, 18, 20, 24, 25, 30, 36\}&\\
\end{flalign*}

\begin{problemparts}
\problempart % Problem 2a
\begin{flalign*}
    E|X| &= \sum_{i = 0}^{3} x_i P(x_i) &&\\
    p &= \frac{1}{2} &&\\
    p(x_i) &= \binom{n}{x} p^{x_i} (1 - p)^{n - x_i} &&\\
    \therefore E|X| &= \frac{3}{2} &&\\
\end{flalign*}
\problempart % Problem 2b
    \begin{flalign*}
    E|Y| &= \sum_{i = 0}^{18} y_i P(y_i) &&\\
    p(Y = 1) &= \frac{1}{36} &\\
    p(Y = 2) &= \frac{2}{36} &\\
    p(Y = 3) &= \frac{2}{36} &\\
    \vdots\\
        E|Y| = \frac{49}{4} &\\
    \end{flalign*}
\problempart % Problem 2c
    \begin{flalign*}
    E|X + Y| &= E|X| + E|Y| = \frac{55}{4} &\\ 
    \end{flalign*}
\end{problemparts}

